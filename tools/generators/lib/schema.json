{
  "$schema": "http://json-schema.org/schema",
  "id": "lib",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Module name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "skipStore": {
      "type": "boolean",
      "description": "Skip store generation"
    }
  },
  "required": ["name"]
}
