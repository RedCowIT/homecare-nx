    "ionic-jobs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "root": "apps/ionic-jobs/",
      "sourceRoot": "apps/ionic-jobs/src",
      "prefix": "hc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "apps/ionic-jobs/www",
            "index": "apps/ionic-jobs/src/index.html",
            "main": "apps/ionic-jobs/src/main.ts",
            "polyfills": "apps/ionic-jobs/src/polyfills.ts",
            "tsConfig": "apps/ionic-jobs/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/ionic-jobs/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              {
                "input": "apps/ionic-jobs/src/theme/variables.scss"
              },
              {
                "input": "apps/ionic-jobs/src/global.scss"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ionic-jobs/src/environments/environment.ts",
                  "with": "apps/ionic-jobs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "ci": {
              "progress": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ionic-jobs:build",
            "port": 8100,
            "host": "homecare.digi-dom.co.uk",
            "sslKey": "./ssl/star-digidom.key",
            "sslCert": "./ssl/STAR_digi-dom_co_uk.crt"
          },
          "configurations": {
            "production": {
              "browserTarget": "ionic-jobs:build:production"
            },
            "ci": {
              "progress": false
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ionic-jobs:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/ionic-jobs/src/test.ts",
            "polyfills": "apps/ionic-jobs/src/polyfills.ts",
            "tsConfig": "apps/ionic-jobs/tsconfig.spec.json",
            "karmaConfig": "apps/ionic-jobs/karma.conf.js",
            "styles": [],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "apps/ionic-jobs/src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "apps/ionic-jobs/src/assets",
                "output": "/assets"
              }
            ]
          },
          "configurations": {
            "ci": {
              "progress": false,
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ionic-jobs/tsconfig.app.json",
              "apps/ionic-jobs/tsconfig.spec.json",
              "apps/ionic-jobs/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "ionic-jobs:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ionic-jobs:build:production"
            }
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "ionic-jobs:ionic-cordova-build",
            "devServerTarget": "ionic-jobs:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "ionic-jobs:ionic-cordova-build:production",
              "devServerTarget": "ionic-jobs:serve:production"
            }
          }
        }
      }
    },
